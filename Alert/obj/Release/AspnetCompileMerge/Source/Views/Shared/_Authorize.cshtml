<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Scripts/plugins/toast/toastr.min.css")
    <script type="text/javascript" src="@Url.Content("~/Scripts/jquery-2.2.3.min.js")"></script>
   <!--@Scripts.Render("~/bundles/jquery")--> 
    <script type="text/javascript" src="@Url.Content("~/Scripts/main.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/plugins/toast/toastr.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/plugins/toast/notification.js")"></script>
</head>
<body class="loginbody">
    <div class="container body-content">
        @RenderBody()
        <footer class="footer">
            <p>&copy; @DateTime.Now.Year - Son Nguyen</p>
        </footer>
    </div>
  
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
