@{
    int i = 0;
}

@foreach (var item in ViewBag.datas)
{
    int member_type = Convert.ToInt32(item.member_type);
    string memberType = "";
    if (member_type == 1)
    {
        memberType = "Member";
    }
    else
    {
        memberType = "Guest";
    }
    
    <tr class="edit"
        id="@item.id" >
        <td class="text-center">
            <input id="@item.id" class="" type="checkbox" value="@item.id" name="keys[]">
        </td>
        <td class="text-center">@(i += 1)</td>
        <td class="memberType">@memberType</td>
        <td class="member">@item.member</td>
        <td class="os_type">@item.os_type</td>
        <td class="longitude">@item.longitude</td>
        <td class="latitude">@item.latitude</td>
        <td class="datecreate">@item.datecreate</td>
        <td class="text-center"><a class="btn btn-info btn-sm viewmap" longitude="@item.longitude" latitude="@item.latitude" data-toggle="modal" data-target="#myModal" href="#"><i class="fa fa-location-arrow" aria-hidden="true"></i></a></td>
        <td></td>
    </tr>
}